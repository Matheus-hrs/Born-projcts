Program Pzim ;

	    var
	    n1,n2,n3,n4,n5,n6,n7,n8,n9,n10 : real;
	    media : real;



 Begin
  
       WRITELN ('Digite o valor 1')  ;
  READLN (n1) ;
  
  
           
       WRITELN ('Digite o valor 2')  ;
  READLN (n2) ;
  
  
       WRITELN ('Digite o valor 3')  ;
  READLN (n3) ;
  
  
       WRITELN ('Digite o valor 4')  ;
  READLN (n4) ;
  
  
       WRITELN ('Digite o valor 5')  ;
  READLN (n5) ;
  
  
       WRITELN ('Digite o valor 6')  ;
  READLN (n6) ;
  
  
       WRITELN ('Digite o valor 7')  ;
  READLN (n7) ;
  
  
       WRITELN ('Digite o valor 8')  ;
  READLN (n8) ;
  
  
       WRITELN ('Digite o valor 9')  ;
  READLN (n9) ;
  
  
       WRITELN ('Digite o valor 10')  ;
  READLN (n10) ;
  
              media := (n1+n2+n3+n4+n5+n6+n7+n8+n9+n10)/10 ;
  
         if ( media > 8) then
         media:=10
         else
         media:=5 ;
  
  
  
  
 End.
